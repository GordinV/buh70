<VFPData>
  <grid>
    <sql>SELECT
              id,
              kuu,
              aasta,
              CASE WHEN kinni = 1
                THEN 'Jah'
              ELSE 'Ei' END :: VARCHAR AS kinni,
              CASE WHEN default_ = 1
                THEN 'JAH'
              ELSE 'EI' END :: VARCHAR AS default_
            FROM ou.aasta Aasta
            WHERE Aasta.rekvid = $1 
            ORDER BY aasta, kuu</sql>
    <alias>curAasta</alias>
  </grid>
